package aquinas;

import java.util.ArrayList;

public class Torre  {

	private ArrayList<Habitacion> torre;
	
	public Torre () {

		
		torre = new ArrayList<Habitacion>();
		torre.add();
	}
	public ArrayList<Habitacion> getTorre (){
		
		return torre;
		
	}
	
	public void anadirHabitacion(Habitacion h){
		
		if (!torre.contains(h)){
		torre.add(h);
		}
		else{
			System.out.println("Esa habitación ya está creada");
		}
	}
	
	public void borrarHabitacion(Habitacion h){
		if(torre.contains(h)){
			torre.remove(h);
		}
		else{
			System.out.println("Está habitación no está creada");
		}
	}
	
	public void inicializarPiso1(){
		
		Habitacion cientouno = new Habitacion (101);
		Habitacion cientodos = new Habitacion (102);
		Habitacion cientotres = new Habitacion (103);
		Habitacion cientocuatro = new Habitacion (104);
		Habitacion cientocinco = new Habitacion (105);
		Habitacion cientoseis = new Habitacion (106);
		Habitacion cientosiete = new Habitacion (107);
		Habitacion cientoocho = new Habitacion (108);
		Habitacion cientonueve = new Habitacion (109);
		Habitacion cientodiez = new Habitacion (110);
		Habitacion cientoonce = new Habitacion (111);
		Habitacion cientodoce = new Habitacion (112);
	}
	
