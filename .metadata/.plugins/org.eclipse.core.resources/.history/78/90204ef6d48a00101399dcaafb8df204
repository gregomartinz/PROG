package aquinas;

import java.util.ArrayList;

public class Torre  {

	private ArrayList<Homologia> torre;
	
	public Torre () {

		
		torre = new ArrayList<Homologia>();
		
	}
	public ArrayList<Homologia> getTorre (){
		
		return torre;
		
	}
	
	public void anadirHabitacion(Homologia h){
		
		if (!torre.contains(h)){
		torre.add(h);
		}
		else{
			System.out.println("Esa habitación ya está creada");
		}
	}
	
	public void borrarHabitacion(Habitacion h){
		if(torre.contains(h)){
			torre.remove(h);
		}
		else{
			System.out.println("Está habitación no está creada");
		}
	}
	
	public void inicializarPiso1(){
		
		Habitacion cientouno = new Habitacion (101);
