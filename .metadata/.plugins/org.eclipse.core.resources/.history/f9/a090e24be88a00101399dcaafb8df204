			Habitacion doscientosdiez = new Habitacion (210,"");
			Habitacion doscientosonce = new Habitacion (211,"");
			Habitacion doscientosdoce = new Habitacion (212,"");
			
			HomHab h21 = new HomHab (doscientosuno,doscientosdoce);
			HomHab h22 = new HomHab (doscientosdos,doscientosonce);
			HomHab h23 = new HomHab (doscientostres,doscientosdiez);
			HomHab h24 = new HomHab (doscientoscuatro,doscientosnueve);
			HomHab h25 = new HomHab (doscientoscinco,doscientosocho);
