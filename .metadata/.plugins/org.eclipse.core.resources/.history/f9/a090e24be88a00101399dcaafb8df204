package aquinas;

import java.util.ArrayList;

public class Torre  {

	private ArrayList<HomHab> torre;
	
	public Torre () {

		
		torre = new ArrayList<HomHab>();
		iniciarTorre();
			
	}
	
	public ArrayList<HomHab> getTorre (){
		
		return torre;
		
	}
	
	public void anadirHomHab(HomHab h){
		
		
		
		if (!torre.contains(h.getH1()) || !torre.contains(h.getH2())){
		
			torre.add(h);
		}
	}
	
	public void borrarHomHab(HomHab h){
		if(torre.contains(h)){
			torre.remove(h);
		}
		else{
			System.out.println("Está habitación no está creada");
		}
	}
	
	public void inicializarPiso1(){
		
		Habitacion cientouno = new Habitacion (101, "");
		Habitacion cientodos = new Habitacion (102,"") ;
		Habitacion cientotres = new Habitacion (103,"");
		Habitacion cientocuatro = new Habitacion (104,"");
		Habitacion cientocinco = new Habitacion (105,"");
		Habitacion cientoseis = new Habitacion (106,"");
		Habitacion cientosiete = new Habitacion (107,"");
		Habitacion cientoocho = new Habitacion (108,"");
		Habitacion cientonueve = new Habitacion (109,"");
		Habitacion cientodiez = new Habitacion (110,"");
		Habitacion cientoonce = new Habitacion (111,"");
		Habitacion cientodoce = new Habitacion (112,"");
		
		HomHab h11 = new HomHab (cientouno,cientodoce);
		HomHab h12 = new HomHab (cientodos,cientoonce);
		HomHab h13 = new HomHab (cientotres,cientodiez);
		HomHab h14 = new HomHab (cientocuatro,cientonueve);
		HomHab h15 = new HomHab (cientocinco,cientoocho);
		HomHab h16 = new HomHab (cientoseis,cientosiete);
		
		torre.add(h11);
		torre.add(h12);
		torre.add(h13);
		torre.add(h14);
		torre.add(h15);
		torre.add(h16);
		
		
	}
	
	public void inicializarPiso2(){
			
			Habitacion doscientosuno = new Habitacion (201,"");
			Habitacion doscientosdos = new Habitacion (202,"");
