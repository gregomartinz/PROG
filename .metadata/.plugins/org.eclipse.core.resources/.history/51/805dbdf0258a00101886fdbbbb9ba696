package es.upm.dit.prog.p5;

import java.io.IOException;

public class Probar {

	/**
	 * @param args
	 * @throws InterruptedException 
	 */
	public static void main(String[] args) throws InterruptedException {
		int i=0;
	
		while(true){
			System.out.println(Math.log1p(Math.sin(Math.sqrt(i))));
			i++;
			Thread.sleep(100);
		}
	}
}
