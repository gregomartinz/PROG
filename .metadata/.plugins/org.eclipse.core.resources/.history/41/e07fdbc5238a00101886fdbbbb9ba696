package es.upm.dit.prog.p5;

import java.io.IOException;

public class Probar {

	/**
	 * @param args
	 * @throws InterruptedException 
	 */
	public static void main(String[] args) throws InterruptedException {
		int i=0;
		for(i=0;i<1000000;i++){
			System.out.println(i);
		}
		while(true){
			System.out.println(i);
			i++;
			Thread.sleep(1000000000);
		}
	}
}
